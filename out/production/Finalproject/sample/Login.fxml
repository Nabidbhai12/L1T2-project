<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Login">
   <children>
      <AnchorPane layoutX="36.0" layoutY="58.0" prefHeight="200.0" prefWidth="200.0" />
      <AnchorPane layoutY="14.0" prefHeight="400.0" prefWidth="658.0" styleClass="bodybg" stylesheets="@pic3.css">
         <children>
            <TextField fx:id="clubnamefield" layoutX="103.0" layoutY="170.0" prefHeight="26.0" prefWidth="371.0" promptText="Enter Club Name">
