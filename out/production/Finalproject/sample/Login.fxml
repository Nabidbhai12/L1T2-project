            <Button fx:id="Exit" layoutX="437.0" layoutY="244.0" mnemonicParsing="false" onAction="#Exitmethod" text="Exit" textFill="#ea1919">
               <font>
                  <Font name="Berlin Sans FB Bold" size="18.0" />
